<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name></display-name>
    <!-- Land服务器 -->
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>Land</servlet-name>
        <servlet-class>Land</servlet-class>
    </servlet>

    <!--Register服务器-->
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>Register</servlet-name>
        <servlet-class>Register</servlet-class>
    </servlet>

    <!--SaveData服务器-->
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>SaveData</servlet-name>
        <servlet-class>SaveData</servlet-class>
    </servlet>

    <!--GetMoney服务器-->
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>GetRank</servlet-name>
        <servlet-class>GetRank</servlet-class>
    </servlet>

    <!--GetData服务器-->
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>GetData</servlet-name>
        <servlet-class>GetData</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>Land</servlet-name>
        <url-pattern>/Land.do</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register.do</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SaveData</servlet-name>
        <url-pattern>/SaveData.do</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetRank</servlet-name>
        <url-pattern>/GetRank.do</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetData</servlet-name>
        <url-pattern>/GetData.do</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>